        <Grid >
            <dxp:DocumentPreviewControl x:Name="reportViewer" >
                <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:FocusBehavior/>
                </dxmvvm:Interaction.Behaviors>
            </dxp:DocumentPreviewControl>

        </Grid>

    </Grid>
</Window>
